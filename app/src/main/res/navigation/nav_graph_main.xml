<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/exploreFragment">

    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.horus.presentation.fragment.ExploreFragment"
        android:label="ExploreFragment"
        tools:layout="@layout/fragment_explore">
        <action
            android:id="@+id/action_exploreFragment_to_hotelFragment"
            app:destination="@id/hotelFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_museumsFragment"
            app:destination="@id/museumsFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_restaurantsFragment"
            app:destination="@id/restaurantsFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_hospitalFragment"
            app:destination="@id/hospitalFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_banksFragment"
            app:destination="@id/banksFragment" />
        <action
            android:id="@+id/action_exploreFragment_to_pyramidsFragment"
            app:destination="@id/pyramidsFragment" />
    </fragment>
    <fragment
        android:id="@+id/communityFragment"
        android:name="com.example.horus.presentation.fragment.CommunityFragment"
        android:label="CommunityFragment"
        tools:layout="@layout/fragment_community"/>
    <fragment
        android:id="@+id/planFragment"
        android:name="com.example.horus.presentation.fragment.PlanFragment"
        android:label="PlanFragment"
        tools:layout="@layout/fragment_plan"/>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.horus.presentation.fragment.SearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/fragment_search"/>
    <fragment
        android:id="@+id/hotelFragment"
        android:name="com.example.horus.presentation.fragment.HotelFragment"
        android:label="fragment_hotel"
        tools:layout="@layout/fragment_hotel" >
        <action
            android:id="@+id/action_hotelFragment_to_exploreFragment"
            app:destination="@id/exploreFragment" />
        <action
            android:id="@+id/action_hotelFragment_to_doubleTreeHotelFragment"
            app:destination="@id/doubleTreeHotelFragment" />
    </fragment>
    <fragment
        android:id="@+id/museumsFragment"
        android:name="com.example.horus.presentation.fragment.MuseumsFragment"
        android:label="fragment_museums"
        tools:layout="@layout/fragment_museums" >
        <action
            android:id="@+id/action_museumsFragment_to_antiquesFragment"
            app:destination="@id/antiquesFragment" />
        <action
            android:id="@+id/action_museumsFragment_to_egyptianMuseumFragment"
            app:destination="@id/egyptianMuseumFragment" />
    </fragment>
    <fragment
        android:id="@+id/restaurantsFragment"
        android:name="com.example.horus.presentation.fragment.RestaurantsFragment"
        android:label="fragment_restaurants"
        tools:layout="@layout/fragment_restaurants" >
        <action
            android:id="@+id/action_restaurantsFragment_to_alexResturantFragment"
            app:destination="@id/alexResturantFragment" />
    </fragment>
    <fragment
        android:id="@+id/hospitalFragment"
        android:name="com.example.horus.presentation.fragment.HospitalFragment"
        android:label="fragment_hospital"
        tools:layout="@layout/fragment_hospital" >
        <action
            android:id="@+id/action_hospitalFragment_to_cancerHospitalFragment"
            app:destination="@id/cancerHospitalFragment" />
    </fragment>
    <fragment
        android:id="@+id/banksFragment"
        android:name="com.example.horus.presentation.fragment.BanksFragment"
        android:label="fragment_banks"
        tools:layout="@layout/fragment_banks" >
        <action
            android:id="@+id/action_banksFragment_to_cairoBankFragment"
            app:destination="@id/cairoBankFragment" />
    </fragment>
    <fragment
        android:id="@+id/antiquesFragment"
        android:name="com.example.horus.presentation.fragment.AntiquesFragment"
        android:label="fragment_antiques"
        tools:layout="@layout/fragment_antiques" />
    <fragment
        android:id="@+id/egyptianMuseumFragment"
        android:name="com.example.horus.presentation.fragment.EgyptianMuseumFragment"
        android:label="fragment_egyptian_museum"
        tools:layout="@layout/fragment_egyptian_museum" />
    <fragment
        android:id="@+id/alexResturantFragment"
        android:name="com.example.horus.presentation.fragment.AlexResturantFragment"
        android:label="fragment_alex_resturant"
        tools:layout="@layout/fragment_alex_resturant" />
    <fragment
        android:id="@+id/cairoBankFragment"
        android:name="com.example.horus.presentation.fragment.CairoBankFragment"
        android:label="fragment_cairo_bank"
        tools:layout="@layout/fragment_cairo_bank" />
    <fragment
        android:id="@+id/doubleTreeHotelFragment"
        android:name="com.example.horus.presentation.fragment.DoubleTreeHotelFragment"
        android:label="fragment_double_tree_hotel"
        tools:layout="@layout/fragment_double_tree_hotel" >
        <action
            android:id="@+id/action_doubleTreeHotelFragment_to_placesReviewFragment"
            app:destination="@id/placesReviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/cancerHospitalFragment"
        android:name="com.example.horus.presentation.fragment.CancerHospitalFragment"
        android:label="fragment_cancer_hospital"
        tools:layout="@layout/fragment_cancer_hospital" />
    <fragment
        android:id="@+id/pyramidsFragment"
        android:name="com.example.horus.presentation.fragment.PyramidsFragment"
        android:label="fragment_pyramids"
        tools:layout="@layout/fragment_pyramids" />
    <fragment
        android:id="@+id/placesReviewFragment"
        android:name="com.example.horus.presentation.fragment.PlacesReviewFragment"
        android:label="fragment_places_review"
        tools:layout="@layout/fragment_places_review" />
</navigation>